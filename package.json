{
  "name": "reaction-tap",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev:web": "pnpm --filter web dev",
    "dev:api": "pnpm --filter api dev",
    "dev": "concurrently -n WEB,API -c auto \"pnpm dev:web\" \"pnpm dev:api\"",
    "lint": "pnpm --filter web lint"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
